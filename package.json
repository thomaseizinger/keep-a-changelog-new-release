{
  "name": "keep-a-changelog-new-release-action",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub action for updating your Keep-A-Changelog CHANGELOG.md for new releases",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "git config core.hooksPath .githooks",
    "build": "webpack --mode production",
    "compile": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test-watch": "vitest watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaseizinger/keep-a-changelog-new-release.git"
  },
  "author": "Thomas Eizinger",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/exec": "^2.0.0",
    "@eslint/js": "^9.39.2",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "stream-buffers": "^3.0.3",
    "typescript-eslint": "^8.49.0",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@types/stream-buffers": "^3.0.8",
    "@types/unist": "^3.0.3",
    "@types/vfile": "^4.0.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.0",
    "eslint-plugin-github": "^6.0.0",
    "js-yaml": "^4.1.1",
    "mock-env": "^0.2.0",
    "prettier": "^3.7.4",
    "to-vfile": "^8.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
